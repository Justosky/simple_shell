#include "shell.h"

/**
*half_len - This function calculates the number of strings in array of strings
*@half: The array of string which length we want to calculate.
*Return: Returns an interger which is the value of the total number of strings
*within the array of strings.
*/

int half_len(char **half)
{
	int count;

	for (count = 0; half[count] != NULL; count++)
	{
	}
	return (count);
}
